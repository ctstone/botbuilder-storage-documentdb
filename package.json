{
  "name": "botbuilder-storage-ddb",
  "version": "1.3.0",
  "description": "IBotStorage implementation for DocumentDB",
  "main": "dist",
  "scripts": {
    "tsc": "tsc",
    "jasmine": "jasmine",
    "istanbul": "istanbul",
    "build": "tsc",
    "test": "istanbul cover --dir %COMMON_TESTRESULTSDIRECTORY%/coverage --report cobertura --report html node_modules/jasmine/bin/jasmine.js",
    "watch": "tsc -w",
    "lint": "tslint -p tsconfig.json",
    "bump": "npm version patch -m \"Bump to %s ***NO_CI***\" ",
    "bump:push": "git push origin HEAD:%BUILD_SOURCEBRANCH% && git push --tags",
    "release:local": "npm pack",
    "preversion": "git config user.name CI && git config user.email %npm_package_author_name%"
  },
  "keywords": [],
  "author": "chstone@microsoft.com",
  "license": "MIT",
  "dependencies": {
    "async": "^2.2.0"
  },
  "peerDependencies": {
    "botbuilder": "^3.7.0",
    "documentdb": "^1.11.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.12",
    "botbuilder": "^3.7.0",
    "documentdb": "^1.11.0",
    "istanbul": "^0.4.5",
    "jasmine": "^2.5.3",
    "jasmine-reporters": "^2.2.1",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2"
  },
  "typings": "dist/index.d.ts"
}
